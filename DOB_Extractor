New-Item -Path '.\Desktop\' -Name "Patient_Name_DOB.txt" -ItemType "file" -Value "" -Force | Out-Null; Select-String -Path '.\Desktop\Unit Patients.htm' -Pattern '(bedfield-text)|(patientfield button)|(patientfield name)|(class=age-)' | ForEach-Object { $_.Line -replace '\n|.*<.*bedfield.*\.|\s\S*D>|.*title=\D?|\".*patientfield.*|\sclass.*','' | Out-File -append -FilePath '.\Desktop\Patient_Name_DOB.txt' }
